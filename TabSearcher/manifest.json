{
    "name": "Tab Search",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "Search and switch between active tabs",
    "icons":{
        "16": "logo16.png",
        "48": "logo48.png"
    },
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {    
            "128": "logo128.png"
        }
    }
}